<!DOCTYPE html>
<html lang='nl'>
<head>
    <meta charset='UTF-8'>
</head>    
<body>
    <h1>Alle studenten</h1>
    <br><br>
    <ul>
        <% students.forEach(function(student){ %>
        <li><span><b>Naam: </b><%= student.naam %></span></li>
        <li><span><b>Geboortedatum: </b><%= student.geboortedatum.toLocaleDateString() %></span></li>
        <li><span><b>Studierichting: </b><%= student.studierichting %></span></li>
        <% }) %>
    </ul>
</body>
</html>